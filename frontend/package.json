{
  "name": "my-pokedex-app",
  "version": "0.1.0",
  "private": true,
  "main": "firebaseFunctions.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "emulator": "firebase emulators:start --project demo-emulator --import .data --export-on-exit --only auth,firestore",
    "deploy": "firebase deploy --only functions,hosting,firestore",
    "logs": "firebase functions:log",
    "lint": "yarn lint:eslint; yarn lint:prettier",
    "lint:eslint": "next lint --dir src",
    "lint:prettier": "prettier --check src",
    "lint:fix": "yarn lint:fix:eslint; yarn lint:fix:prettier",
    "lint:fix:eslint": "next lint --fix --dir src",
    "lint:fix:prettier": "prettier --write src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -c .storybook",
    "ci:vrt": "reg-suit run",
    "ci:storybook-generate": "storybook build -c .storybook -o dist-storybook -s public",
    "ci:storycap": "storycap --serverTimeout 30000 --captureTimeout 10000 --serverCmd 'npx http-server dist-storybook --ci -p 9009' http://127.0.0.1:9009"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "2.7.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@next/bundle-analyzer": "^13.4.12",
    "@tanstack/react-query": "^4.27.0",
    "@tanstack/react-virtual": "^3.0.0-beta.54",
    "@vercel/og": "^0.5.9",
    "chakra-react-select": "^4.6.0",
    "chartjs-plugin-doughnutlabel": "^2.0.3",
    "date-fns": "^2.30.0",
    "firebase": "^10.1.0",
    "firebase-admin": "^11.10.1",
    "firebase-auth": "^0.1.2",
    "firebase-functions": "^4.4.1",
    "framer-motion": "^10.6.0",
    "next": "13.2",
    "react": "18.2.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.9.0",
    "react-select": "^5.7.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.3.2",
    "@storybook/nextjs": "^7.3.2",
    "@storybook/react": "^7.3.2",
    "@storybook/testing-library": "^0.2.0",
    "@svgr/webpack": "^6.5.1",
    "@types/node": "^18",
    "@types/react": "^18.2.15",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-chakra-ui": "^0.10.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-unused-imports": "^2.0.0",
    "firebase-functions-test": "^3.1.0",
    "firebase-tools": "^12.4.5",
    "plop": "^3.1.2",
    "prettier": "^2.8.8",
    "puppeteer": "^21.1.0",
    "reg-keygen-git-hash-plugin": "^0.12.2",
    "reg-notify-github-plugin": "^0.12.2",
    "reg-publish-gcs-plugin": "^0.12.2",
    "reg-suit": "^0.12.2",
    "storybook": "^7.3.2",
    "storycap": "^4.2.0",
    "typescript": "5.1.6"
  }
}
